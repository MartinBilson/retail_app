<!DOCTYPE html>
<html lang="en" >
<head>
	<!-- Mobile Specific Meta -->
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<!-- Favicon-->
	<link rel="shortcut icon" href="<%= static_path(@conn, "/images/fav.ico") %>">
	<!-- Author Meta -->
	<meta name="author" content="GTS,CS Digital">
	<!-- Meta Description -->
	<meta name="description" content="Thamani online is one of the seamless retail platform to provide real time payment to the parties within the system ">
	<!-- Meta Keyword -->
	<meta name="keywords" content="Realtime payment, Thamani, Thamani Kenya, ThamaniKenya, Thamani Software,Thamani online, Thamanionline, Kenya, online supply chain, Seasmless Retail Platform, Retail Platform,  Manufacturer, Warehousing, Data Synchronisation">
	<!-- meta character set -->
	<meta charset="UTF-8">
	<!-- Site Title -->
	<title>Thamani Online</title>

		  <link href='https://fonts.googleapis.com/css?family=Muli:400,300' rel='stylesheet' type='text/css'>
		<!--
		CSS
		============================================= -->
		<link rel="stylesheet" href="<%= static_path(@conn, "/css/linearicons.css") %>">
		<link rel="stylesheet" href="<%= static_path(@conn, "/css/owl.carousel.css") %>">
		<link rel="stylesheet" href="<%= static_path(@conn, "/css/font-awesome.min.css") %>">
		<link rel="stylesheet" href="<%= static_path(@conn, "/css/nice-select.css") %>">
		<link rel="stylesheet" href="<%= static_path(@conn, "/css/magnific-popup.css") %>">
		<link rel="stylesheet" href="<%= static_path(@conn, "/css/bootstrap.css") %>">
		<link rel="stylesheet" href="<%= static_path(@conn, "/css/main.css") %>">
			<link rel="stylesheet" href="<%= static_path(@conn, "/css/cookieconsent.min.css") %>">
		<script src="<%= static_path(@conn, "/js/cookieconsent.min.js") %>"></script>


	  <script language="javascript" type="text/javascript">
	window.addEventListener("load", function(){
		window.cookieconsent.initialise({
     container: document.getElementById("content"),
		 "palette": {
 	    "popup": {
 	      "background": "rgba(239, 0, 29, 0.98)"
	      	    
		},
 	    "button": {
 	      "background": "#f1d600"
 	    },
			"content": {
			  "header": 'Cookies used on the website!',
			  "message": 'This website uses cookies to ensure you get the best experience on our website.niubyvtcryvuhijovgbuhn',
			  "dismiss": 'Got itbnuiebds!',
			  "allow": 'Allow cookies',
			  "deny": 'Decline',
			  "link": 'Learn more',
			  "href": 'http://cookiesandyou.com',
			  "close": '&#x274c;',
			  "policy": 'Cookie Policy',
			  "target": '_blank',
			},
     },

   })});
	</script>

	</head>
	<body>
		<div class="main-wrapper-first">
			<header>
				<div class="container">
					<div class="header-wrap">
						<div class="header-top d-flex justify-content-between align-items-center">
							<div class="logo">
								<a href="#"><img src="<%= static_path(@conn, "/images/thamani_logo.png") %>" width ="195px;" alt=""></a>
							</div>

							<nav class="">
								<p style="display:inline-block"><span style= "color:#ef001d;margin-left:15px;">Secure <i class="fa fa-lock" style= "color:#00bf48"></i></span></br><%= link "Login", to: login_path(@conn, :index), style: "color:;" %></p>

								<p style="line-height:25px;"class="primary-btn hover align-items-center"><span style= "margin-left:15px;">New account?</span></br><span><%= link "register", to: register_path(@conn, :index), style: "color:#fff;" %></span></p>

							</nav>

						</div>
					</div>
				</div>
			</header>
		</div>

      </header>



      <main role="main">
        <%= render @view_module, @view_template, assigns %>
      </main>

    </div> <!-- /container -->
    <!-- Start Footer Widget Area -->
    <section class="footer-widget-area">
      <div class="container">
        <div class="row">
          <div class="col-md-3">
            <div class="single-widget d-flex flex-wrap justify-content-between">
              <div class="icon d-flex align-items-center justify-content-center">
                <span class="lnr lnr-earth"></span>
              </div>
              <div class="desc">
                <h6 class="title text-uppercase">Email Address</h6>
                <div class="contact">
                  <a href="mailto:info@thamanionline.com">info@thamanionline.com</a> <br>
                  <a href="mailto:support@thamanionline.com">support@thamanionline.com</a>
                </div>
              </div>
            </div>
          </div>
          <div class="col-md-3">
            <div class="single-widget d-flex flex-wrap justify-content-between">
              <div class="icon d-flex align-items-center justify-content-center">
                <span class="lnr lnr-phone"></span>
              </div>
              <div class="desc">
                <h6 class="title text-uppercase">Phone Number</h6>
                <div class="contact">
                  <a href="tel:020 238 5270">020 238 5270</a> <br>
                  <a href="tel:020 231 8414">020 231 8414</a>
                </div>
              </div>
            </div>
          </div>
          <div class="col-md-3">
            <div class="single-widget d-flex flex-wrap justify-content-between">
              <div class="icon d-flex align-items-center justify-content-center">
                <span class="lnr lnr-pushpin"></span>
              </div>
              <div class="desc">
                <h6 class="title text-uppercase">Extra </h6>
                <p><%= link "Privacy Policy", to: privacy_path(@conn, :index), style: "color:#fff;" %> 
		<br> <%= link "Terms & Conditions", to: terms_path(@conn, :index), style: "color:#fff;" %> 
		<br> <%= link "Login", to: login_path(@conn, :index), style: "color:#fff;" %> 
		<br> 	<%= link "Register", to: register_path(@conn, :index), style: "color:#fff;" %></p>
              </div>
            </div>
          </div>
        </div>
      </div>
      <footer>
        <div class="container">
          <div class="footer-content d-flex justify-content-between align-items-center flex-wrap">
            <div class="logo">
              <a href="#"><img src="<%= static_path(@conn, "/images/thamani_logo.png") %>" width ="155px;" alt=""></a>
            </div>
            <div class="copy-right-text"> &copy;
           <script>
               document.write(new Date().getFullYear())
           </script> Thamani Online.All rights reserved.</div>
            <div class="footer-social">
              <a href="#"><i class="fa fa-facebook"></i></a>
              <a href="#"><i class="fa fa-twitter"></i></a>
              <a href="#"><i class="fa fa-instagram"></i></a>

            </div>
          </div>
        </div>
      </footer>
    </section>
    <!-- End Footer Widget Area -->

  </div>




  <script src="<%= static_path(@conn, "/js/vendor/jquery-2.2.4.min.js") %>"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous"></script>
  <script src="<%= static_path(@conn, "/js/vendor/bootstrap.min.js") %>"></script>
  <script src="<%= static_path(@conn, "/js/jquery.ajaxchimp.min.js") %>"></script>
  <script src="<%= static_path(@conn, "/js/owl.carousel.min.js") %>"></script>
  <script src="<%= static_path(@conn, "/js/jquery.nice-select.min.js") %>"></script>
  <script src="<%= static_path(@conn, "/js/jquery.magnific-popup.min.js") %>"></script>
  <script src="<%= static_path(@conn, "/js/main.js") %>"></script>
  </body>
  </html>
