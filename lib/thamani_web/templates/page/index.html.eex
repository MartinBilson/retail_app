  <!-- End Feature Area -->
  <!-- Start Story Area -->
  <section class="story-area">
    <div class="container">
      <div class="row align-items-center">
        <div class="col-lg-3">
          <div class="story-title">
            <h3 class="text-white">Why</h3>
            <span class="text-uppercase text-white">Thamani Online</span>
          </div>
        </div>
        <div class="col-lg-6">
          <div class="story-box">
            <h6 class="text-uppercase">Why Thamani Online</h6>
            <p>- Realtime Settlement and data capture along the supply chain</p>
            <p>- Cashless system of buying goods</p>
            <p>- Reduced risk along the supply chain</p>
            <p>- Online reorder of stocks</p>

          </div>
        </div>
      </div>
    </div>
  </section>
  <!-- End Story Area -->

  <!-- Start Story Area -->
  <section class="story-area-2">
    <div class="container">
      <div class="row align-items-center">
        <div class="col-lg-3">
          <div class="story-title sm-show text-center">
            <h3 class="text-white">How</h3>
            <span class="text-uppercase text-white">Thamani Online</span>
          </div>
        </div>
        <div class="col-lg-6">
          <div class="story-box">
            <h6 class="text-uppercase">Benefits of thamani Online</h6>
            <p>- Delivery of goods is simplified</p>
            <p>- Supplier Payment is digitised and real time</p>
            <p>- Stock shrinkage is captured real time and compensated</p>
            <p>- No need of book keeping and reconcciliation</p>
          </div>
        </div>
        <div class="col-lg-3">
          <div class="story-title sm-hide text-right">
            <h3 class="text-white">Benefits </h3>
            <span class="text-uppercase text-white">Of The supply chain Works</span>
          </div>
        </div>
      </div>
    </div>
  </section>
