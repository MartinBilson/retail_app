

                                            <div class="row">
                                                                   <div class="col-md-12">

                                                                       <div class="card">
                                                                           <div class="card-header" data-background-color="gold">
                                                                               <h4 class="title">Return From Retailer</h4>
                                                                               <p class="category"><%= link "Dashboard", to: manufacturer_admin_dashboard_path(@conn, :index) %>/return</p>

                                                                           </div>

                                                                           <div class="card-content table-responsive">
                                                                             <div id="toolbar" style="margin-top: -16px;">
                                                                            <select class="form-control">
                                                                              <option value="">Export Current page</option>
                                                                              <option value="all">Export All</option>
                                                                              <option value="selected">Export Selected</option>
                                                                            </select>
                                                                          </div>
                                          <table class="table" id ="fresh-table" data-toggle="table" data-toolbar="#toolbar" data-search="true"  data-pagination="true" data-show-print="true" data-show-export="true"   data-export-options='{"fileName": "reports"}'>
                                              <thead class="text-info">
                                                <tr>
                                                  <th data-field="state" data-checkbox="true"></th>
                                                  <th data-field="item" data-sortable="true">SKU</th>
                                                  <th data-field="company" data-sortable="true">From</th>
                                                  <th data-field="co" data-sortable="true">To</th>
                                                  <th data-field="quantity" data-sortable="true">Quantity</th>
                                                  <th data-field="production" data-sortable="true">Description</th>

                                                  <th class="text-center" >Issue Status</th>
                                                  <th class="text-center"> Actions</th>
                                                </tr>
                                              </thead>
                                              <tbody>
                                            <%= for recieved <- @recieved do %>
                                                <tr>
                                                  <td></td>
                                                  <td><%= recieved.sku.gtin %>(<%= recieved.sku.name %>) </td>
                                                  <td><%= recieved.user.company %> </td>
                                                  <td><%= recieved.companies.company %> </td>
                                                  <td><%= recieved.quantity %> </td>
                                                  <td><%= recieved.description %> </td>



                                                  <%= if recieved.active == "true" do %>
                                                    <td><i class="fa fa-check" style= "color:#66bb6a !important"></i></td>
                                                  <% else  %>
                                                  <td><i class="fa fa-close" style= "color:#f44336 !important"></i></td>
                                                  <% end %>
                                                  <td class="text-right">
                                                  <span  rel="tooltip" title="View Task" class="btn btn-info btn-simple ">
                                                      <span ><%= link "",  to: rreturn_admin_path(@conn, :show, recieved), class: "fa fa-search-plus" ,style: "color:#00bcd4 !important" %></span>
                                                  </span>



</td>

    </tr>
<% end %>
  </tbody>
</table>
</div>
                                                        </div>
                                                      </div>
                                                    </div>
